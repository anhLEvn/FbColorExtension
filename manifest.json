{
    "name": "Facebook color Extension",
    "description": "Extension to change FB Color",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://*.facebook.com/","https://*.facebook.com/*" ],
        "css": ["css/styles.css"],
        "js": ["js/custom.js"]
      }
    ]
  }